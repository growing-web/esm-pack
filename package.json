{
  "name": "@growing-web/esmpack",
  "version": "0.1.0",
  "description": "esm pack.",
  "keywords": [
    "growing-web",
    "cdn",
    "esmpack"
  ],
  "homepage": "https://github.com/growing-web/esm-pack#readme",
  "bugs": {
    "url": "https://github.com/growing-web/esm-pack/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/growing-web/esm-pack.git"
  },
  "license": "UNLICENSED",
  "author": "vben",
  "scripts": {
    "prebuild": "pnpm rimraf dist",
    "build": "pnpm cross-env NODE_ENV=production nest build",
    "dev": "pnpm cross-env  yarn start:dev",
    "pm2:restart": "npm run build && pm2 reload ecosystem.config.js",
    "pm2:start": "npm run build && pm2 startOrRestart ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "prepublishOnly": "npm run build",
    "start": "pnpm cross-env NODE_ENV=development nest start",
    "start:debug": "pnpm cross-env NODE_ENV=development nest start  --watch",
    "start:dev": "pnpm cross-env NODE_ENV=development nest start --watch",
    "start:prod": "node dist/main.js",
    "changeset": "changeset",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "husky install",
    "lint": "eslint .  --fix",
    "release": "pnpm run build && changeset publish",
    "stub": "pnpm run prepack --filter ./packages -- --stub",
    "test": "vitest",
    "version": "pnpm run changeset version"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ],
    "packages/**/*.d.ts": [
      "eslint --ext .ts"
    ],
    "packages/**/*.ts": [
      "eslint --ext .ts"
    ]
  },
  "dependencies": {
    "@growing-web/esm-pack-core": "^0.2.3",
    "@hapi/joi": "17.1.1",
    "@nestjs/common": "^8.4.5",
    "@nestjs/config": "2.0.0",
    "@nestjs/core": "^8.4.5",
    "@nestjs/platform-express": "^8.4.5",
    "@nestjs/throttler": "2.0.1",
    "async-lock": "^1.3.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "compression": "1.7.4",
    "dayjs": "1.11.2",
    "debug": "4.3.4",
    "dotenv": "16.0.1",
    "esbuild": "^0.14.39",
    "etag": "^1.8.1",
    "fast-glob": "3.2.11",
    "fs-extra": "^10.1.0",
    "helmet": "5.1.0",
    "lodash": "4.17.21",
    "lru-cache": "^7.10.1",
    "magic-string": "^0.26.2",
    "mime": "^3.0.0",
    "morgan": "^1.10.0",
    "npm-registry-fetch": "^13.1.1",
    "picocolors": "^1.0.0",
    "pkg-types": "^0.3.2",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "request-progress": "^3.0.0",
    "request-promise": "^4.2.6",
    "resolve": "^1.22.0",
    "rxjs": "^7.5.5",
    "semver": "^7.3.7",
    "sri-toolbox": "^0.2.0",
    "stacktrace-js": "^2.0.2",
    "sver": "^1.8.3",
    "tar": "^6.1.11",
    "validate-npm-package-name": "^4.0.0",
    "winston": "3.7.2",
    "winston-daily-rotate-file": "4.6.1"
  },
  "devDependencies": {
    "@changesets/cli": "^2.22.0",
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@nestjs/cli": "^8.2.5",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/testing": "^8.4.5",
    "@types/async-lock": "^1.1.5",
    "@types/compression": "^1.7.2",
    "@types/debug": "4.1.7",
    "@types/express": "^4.17.13",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "4.14.182",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.34",
    "@types/npm-registry-fetch": "^8.0.4",
    "@types/request-promise": "^4.1.48",
    "@types/semver": "7.3.9",
    "@types/tar": "^6.1.1",
    "@types/validate-npm-package-name": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "cross-env": "7.0.3",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "^1.4.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "nodemon": "2.0.16",
    "picocolors": "^1.0.0",
    "pm2": "5.2.0",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.6.4",
    "unbuild": "^0.7.4",
    "vite": "^2.9.9",
    "vitest": "^0.12.6"
  },
  "engines": {
    "node": ">=14"
  }
}
