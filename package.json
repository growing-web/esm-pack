{
  "name": "growing-web-cdn-server-monorepo",
  "version": "0.0.1",
  "private": true,
  "description": "growing-web-cdn-server-monorepo.",
  "keywords": [
    "growing-web",
    "cdn",
    "server"
  ],
  "homepage": "https://github.com/growing-web/cdn-server#readme",
  "bugs": {
    "url": "https://github.com/growing-web/cdn-server/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/growing-web/cdn-server.git"
  },
  "license": "UNLICENSED",
  "author": "vben",
  "scripts": {
    "build": "turbo run build",
    "changeset": "changeset",
    "clean": "turbo run clean && rimraf node_modules",
    "dev": "pnpm run dev --filter scripts",
    "dev:all": "turbo run dev",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "husky install",
    "lint": "eslint .  --fix",
    "release": "pnpm run build && changeset publish",
    "stub": "pnpm run prepack --filter ./packages -- --stub",
    "test": "vitest",
    "test:coverage": "c8 vitest --coverage",
    "test:watch": "vitest -w",
    "version": "pnpm run changeset version"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ],
    "packages/**/*.d.ts": [
      "eslint --ext .ts"
    ],
    "packages/**/*.ts": [
      "eslint --ext .ts"
    ]
  },
  "devDependencies": {
    "@changesets/cli": "^2.22.0",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "@vitest/ui": "^0.8.1",
    "c8": "^7.11.0",
    "cross-env": "7.0.3",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-define-config": "^1.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-standard": "^5.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "picocolors": "^1.0.0",
    "prettier": "^2.6.1",
    "rimraf": "^3.0.2",
    "turbo": "^1.1.10",
    "typescript": "^4.6.3",
    "unbuild": "^0.7.2",
    "vite": "^2.9.1",
    "vitest": "^0.8.1"
  },
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "es-module-lexer": "^0.10.4"
  }
}
