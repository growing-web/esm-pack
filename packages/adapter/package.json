{
  "name": "@growing-web/esmpack-adapter",
  "version": "0.1.1",
  "description": "esmpack adapter.",
  "keywords": [
    "growing-web",
    "cdn",
    "esmpack",
    "adapter"
  ],
  "license": "MIT",
  "author": "vben",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
