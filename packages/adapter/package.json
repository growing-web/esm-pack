    "stub": "npm run dev"
  },
  "dependencies": {
    "@types/ali-oss": "^6.16.3",
    "@types/lodash": "4.14.182",
    "@types/mime": "^2.0.3",
    "ali-oss": "^6.17.1",
    "fast-glob": "3.2.11",
